{
  "hosting": {
    "public": "dist/tkdtorneos/browser",
    "rewrite": {
      "source": "**",
      "destination": "/index.html"
    },
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "extensions": {}
}
